--><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="WebApp_ID" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <servlet>
    <servlet-name>GcmServlet</servlet-name>
    <servlet-class>
      org.onepf.opfpush.GcmServlet
    </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet>
    <servlet-name>GcmRegisterServlet</servlet-name>
    <servlet-class>
      org.onepf.opfpush.GcmRegisterServlet
    </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet>
    <servlet-name>AdmServlet</servlet-name>
    <servlet-class>
      org.onepf.opfpush.AdmServlet
    </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet>
    <servlet-name>AdmRegisterServlet</servlet-name>
    <servlet-class>
      org.onepf.opfpush.AdmRegisterServlet
    </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet>
    <servlet-name>NokiaServlet</servlet-name>
    <servlet-class>
      org.onepf.opfpush.NokiaServlet
    </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet>
    <servlet-name>NokiaRegisterServlet</servlet-name>
    <servlet-class>
      org.onepf.opfpush.NokiaRegisterServlet
    </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>GcmServlet</servlet-name>
    <url-pattern>/gcm</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>GcmRegisterServlet</servlet-name>
    <url-pattern>/gcmregister</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AdmServlet</servlet-name>
    <url-pattern>/adm</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AdmRegisterServlet</servlet-name>
    <url-pattern>/admregister</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>NokiaServlet</servlet-name>
    <url-pattern>/nokia</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>NokiaRegisterServlet</servlet-name>
    <url-pattern>/nokiaregister</url-pattern>
  </servlet-mapping>

</web-app>